<Window x:Class="Presto.SWCamp.Lyrics.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Presto.SWCamp.Lyrics"
        mc:Ignorable="d" AllowsTransparency="True" WindowStyle="None" Foreground="black"
        Title="Window2" Height="500" Width="430">
    <Window.Background>
        <SolidColorBrush Color="Black" Opacity="0"/>
    </Window.Background>
    <Grid x:Name="MainGrid" HorizontalAlignment="Center" Height="500" VerticalAlignment="Top" Width="430">
        <Border CornerRadius="5" Background="black" Opacity="0.65" MinHeight="250">
        </Border>
        <StackPanel 
            x:Name="stkPanel" 
            HorizontalAlignment="Center" VerticalAlignment="Top"
            Margin="0 10 0 10"
            >
        </StackPanel>
    </Grid>
</Window>
